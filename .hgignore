syntax: regexp
## Ephermeral files
~$                          # gedit
py[co]$                     # compiled python files
__pycache__/
\.rej$                      # hg revert rejected changes
\.orig$                     # hg revert backups
\.profile$                  # profiling output files
\.out(put)?$                # profiling output files
profiles/                   # profiling output files
tests?/\w*[-_]?data/        # data files generated for testing purposes
\.7z
\.tar\.

## Packaging
\.egg-info/
^dist/

## Local
^data/
\.sublime.*$                # Sublime Text project management
venv[^/]*/                  # virtualenvs (any folder prefixed with "venv")
multiworm_settings.py$      # local environment settings

## Builds
_build/                     # Sphinx
build/                      # sometimes pip
